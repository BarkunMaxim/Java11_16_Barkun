package by.tr.xmlAnalized;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;

import by.tr.xmlAnalized.service.AnalizedService;
import by.tr.xmlAnalized.service.impl.AnalizedServiceImpl;

public class xmlAnalized {

	public static void main(String[] args) {
		AnalizedService analizedService = new AnalizedServiceImpl();
		BufferedReader bufferedReader = null;
		try{bufferedReader = new BufferedReader(new FileReader(".//src//resource//notes.xml"));
		int symbol = bufferedReader.read();
		while(symbol !=-1){
			System.out.println((char)bufferedReader.read());
		
		}
		bufferedReader.close();
		
	}finally {
        if (bufferedReader != null) {
            try {
            	bufferedReader.close();
            } catch (IOException e) {
                e.printStackTrace();
            }
        }
		//analizedService.getNode(".//src//resource//notes.xml");
	}
		
	}
}
